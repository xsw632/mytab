{
  "manifest_version": 3,
  "name": "MyTab - 新标签页",
  "version": "1.0.0",
  "description": "一个美观、功能丰富的新标签页扩展，支持自定义壁纸、快捷链接和分类管理",
  "author": "MyTab",
  "icons": {
    "16": "images/icon16.svg",
    "48": "images/icon48.svg",
    "128": "images/icon128.svg"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.svg",
      "48": "images/icon48.svg"
    },
    "default_title": "MyTab 设置"
  },
  "permissions": [
    "storage"
  ]
}